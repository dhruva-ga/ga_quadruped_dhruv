        <mesh name="thigh_link3_1" content_type="model/stl" file="thigh_link3_1.stl" scale="0.001 0.001 0.001" />
        <mesh name="calf_link1_1" content_type="model/stl" file="calf_link1_1.stl" scale="0.001 0.001 0.001" />
        <mesh name="calf_link4_1" content_type="model/stl" file="calf_link4_1.stl" scale="0.001 0.001 0.001" />
